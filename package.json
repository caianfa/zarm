{
  "name": "zarm",
  "version": "2.0.0-alpha.35",
  "description": "基于 React 的移动端UI库",
  "tags": [
    "zarm",
    "zarm-ui",
    "react",
    "react-zarm",
    "zhongan"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "zarm",
    "zarm-ui",
    "zarm-mobile",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "mobile"
  ],
  "license": "MIT",
  "repository": "git://github.com/ZhonganTechENG/zarm",
  "bugs": {
    "url": "https://github.com/ZhonganTechENG/zarm/issues"
  },
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf types lib es dist assets coverage components/**/*.js components/**/**/*.js examples-rn/**/build examples-rn/**/bundle",
    "dev": "webpack-dev-server --config scripts/webpack/config.dev.js",
    "dev:ios": "concurrently \"npm run build:rn -- -w\" \"react-native run-ios --project-path examples-rn/ios\"",
    "dev:android": "concurrently \"npm run build:rn -- -w\" \"react-native run-android --root examples-rn\"",
    "build:types": "rimraf types && tsc --outDir types -d --emitDeclarationOnly",
    "build:es": "rimraf es && babel components -d es -m es6 --extensions \".ts,.tsx\" --copy-files --presets @babel/typescript,@babel/react --plugins @babel/plugin-proposal-class-properties,@babel/plugin-proposal-object-rest-spread",
    "build:lib": "rimraf lib && babel components -d lib -m commonjs --extensions \".ts,.tsx\" --copy-files --presets @babel/env,@babel/react,@babel/typescript --plugins @babel/plugin-proposal-class-properties",
    "build:umd": "rimraf dist && webpack --config scripts/webpack/config.umd.js && cross-env NODE_ENV=production webpack --config scripts/webpack/config.umd.js",
    "build:css": "rimraf lib/**/style/css.js lib/***/style/*.css es/**/style/css.js es/**/style/*.css && cd scripts/gulp && gulp",
    "build:ios": "concurrently \"node scripts/native/mkdir\" \"npm run build:rn && node node_modules/react-native/local-cli/cli.js bundle --platform ios --dev false --entry-file examples-rn/index.js --bundle-output examples-rn/ios/bundle/index.bundle --assets-dest examples-rn/ios/bundle\"",
    "build:android": "concurrently \"node scripts/native/mkdir\" \"npm run build:rn && node node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file examples-rn/index.js --bundle-output examples-rn/android/bundle/index.bundle --assets-dest examples-rn/android/bundle\"",
    "build:rn": "rimraf components/**/*.js components/**/**/*.js && babel components -d components -m es6 --jsx react-native --extensions \".ts,.tsx\" --presets @babel/react,@babel/typescript --plugins @babel/plugin-proposal-class-properties,@babel/plugin-proposal-object-rest-spread,@babel/plugin-transform-react-jsx",
    "build": "npm run build:types && npm run build:lib && npm run build:es && npm run build:umd && npm run build:css",
    "deploy:build": "rimraf assets && webpack --config scripts/webpack/config.deploy.js",
    "deploy": "npm run deploy:build && touch assets/CNAME && echo 'zarm.design' > assets/CNAME && gh-pages -d assets",
    "lint:ts": "eslint --ext .ts,.tsx components",
    "lint:js": "eslint --ext .js,.jsx site examples-rn",
    "lint:style": "stylelint --syntax scss components/**/*.scss components/**/**/*.scss site/styles/**/*.scss",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:style",
    "test:h5": "cross-env TZ=America/New_York jest --config scripts/jest/config.js",
    "test:rn": "jest --config scripts/jest/config.native.js",
    "test": "rimraf coverage && npm run test:h5 -- --coverage && cp ./coverage/coverage-final.json ./coverage/coverage-h5-final.json && npm run test:rn -- --coverage && cp ./coverage/coverage-final.json ./coverage/coverage-rn-final.json && node ./scripts/jest/mapCoverage.js",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -w -r 0",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "bash scripts/release.sh"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "dependencies": {
    "better-scroll": "1.15.1",
    "classnames": "2.2.6",
    "create-react-context": "0.2.3",
    "hoist-non-react-statics": "^3.3.0",
    "normalize.css": "8.0.1",
    "react-native-camera-roll-picker": "1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.2",
    "@babel/runtime-corejs2": "^7.4.2",
    "@babel/standalone": "^7.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/classnames": "^2.2.3",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/jest": "^23.1.4",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/react-native": "^0.51.9",
    "@typescript-eslint/eslint-plugin": "^1.7.1-alpha",
    "autoprefixer": "^9.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-native": "^4.0.0",
    "browserslist": "^4.4.1",
    "color": "^3.1.0",
    "concurrently": "^3.5.1",
    "conventional-changelog-cli": "^1.3.22",
    "core-js": "^3.0.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.1",
    "css-vars-ponyfill": "^2.0.2",
    "docsearch.js": "^2.6.1",
    "dragon-ui": "1.0.0-alpha.18",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-za": "^2.0.0-alpha.7",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fastclick": "^1.0.6",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^1.0.3",
    "gh-pages": "^1.0.0",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "lint-staged": "^4.3.0",
    "marked": "^0.4.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "qrious": "^4.0.2",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.8.0",
    "react-loadable": "^5.5.0",
    "react-native": "^0.55.2",
    "react-native-mock-render": "0.0.27",
    "react-navigation": "^2.9.2",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "through2": "^3.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "lint-staged": {
    "components/**/*.{ts,tsx}": "eslint",
    "{site,examples-rn}/**/*.{js,jsx}": "eslint",
    "{styles,site}/**/*.scss": "stylelint"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 9",
    "iOS >= 7",
    "Android >= 4"
  ],
  "sideEffects": [
    "dist/*",
    "components/style/*",
    "components/**/style/*",
    "es/**/style/*",
    "lib/**/style/*",
    "site/**/**.scss"
  ]
}
